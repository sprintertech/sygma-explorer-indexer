{
    "ignore": [
      "*.txt",
      "./data",
      ".github/",
      ".idea",
      "*.yml",
      "*.yaml",
      "README.md",
      "prisma/",
      ".dockerignore",
      ".gitignore",
      ".prettierrc",
      ".env",
      ".env.**",
      "yarn.lock",
      "public/",
      "jest.config.js",
      ".eslintrc.**",
      "./scripts/"
    ],
    "license": "header.txt",
    "licenseFormats": {
      "js": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "dotfile|^Dockerfile|api": {
        "eachLine": {
          "prepend": "# "
        }
      }
    },
    "trailingWhitespace": "TRIM"
}